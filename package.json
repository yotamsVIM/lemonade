{
  "name": "lemonade-ehr-integration",
  "version": "1.0.0",
  "description": "Automated AI-EHR Integration Platform",
  "main": "dist/backend/server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --exec tsx src/backend/server.ts",
    "build": "tsc",
    "start": "node dist/backend/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": ["ehr", "ai", "automation", "healthcare"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express-async-errors": "^3.1.1",
    "@google/generative-ai": "^0.1.3"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/cors": "^2.8.17",
    "vitest": "^1.1.0",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2",
    "playwright": "^1.40.1",
    "@playwright/test": "^1.40.1",
    "tsx": "^4.7.0",
    "nodemon": "^3.0.2",
    "mongodb-memory-server": "^9.1.5"
  }
}
