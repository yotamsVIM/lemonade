{
  "name": "lemonade-ehr-integration",
  "version": "1.0.0",
  "description": "Automated AI-EHR Integration Platform",
  "main": "dist/backend/server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --exec tsx src/backend/server.ts",
    "dev:frontend": "vite",
    "build": "tsc && vite build",
    "build:backend": "tsc",
    "build:frontend": "vite build",
    "start": "node dist/backend/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "ehr",
    "ai",
    "automation",
    "healthcare"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.1.3",
    "@langchain/core": "^1.1.12",
    "@langchain/google-genai": "^2.1.7",
    "@langchain/langgraph": "^1.0.15",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "langchain": "^1.2.7",
    "mongoose": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/supertest": "^6.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "mongodb-memory-server": "^9.1.5",
    "nodemon": "^3.0.2",
    "playwright": "^1.40.1",
    "supertest": "^6.3.3",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  }
}
