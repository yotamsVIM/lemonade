{
  "name": "lemonade-ehr-integration",
  "version": "1.0.0",
  "description": "Automated AI-EHR Integration Platform",
  "main": "dist/backend/server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "AWS_PROFILE=ai-developer nodemon --exec tsx src/backend/server.ts",
    "dev:frontend": "vite",
    "dev:forge": "AWS_PROFILE=ai-developer nodemon --exec tsx src/forge/index.ts",
    "build": "tsc && vite build",
    "build:backend": "tsc",
    "build:frontend": "vite build",
    "start": "AWS_PROFILE=ai-developer node dist/backend/server.js",
    "start:forge": "AWS_PROFILE=ai-developer node dist/forge/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:forge": "vitest run tests/forge.spec.ts",
    "test:extension": "playwright test",
    "test:extension:ui": "playwright test --ui",
    "test:extension:headed": "playwright test --headed",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "ehr",
    "ai",
    "automation",
    "healthcare"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/aws": "^0.1.0",
    "@langchain/core": "^0.3.80",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "htmlparser2": "^9.1.0",
    "mongoose": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiktoken": "^1.0.18",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/supertest": "^6.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "mongodb-memory-server": "^9.1.5",
    "nodemon": "^3.0.2",
    "playwright": "^1.40.1",
    "supertest": "^6.3.3",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  }
}
